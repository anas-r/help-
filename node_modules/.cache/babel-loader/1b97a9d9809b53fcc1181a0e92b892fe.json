updatedWindow = response.data;\n      this.$emit('window-updated', updatedWindow);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.open-status {\n  .icon {\n    position: relative;\n  }\n\n  &.open {\n    color: #198754;\n    .icon {\n      font-size: 12px;\n      top: -3px;\n    }\n  }\n\n  &.closed {\n    color: #dc3545;\n  }\n}\n\n.window {\n  .top-row {\n    cursor: pointer;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}